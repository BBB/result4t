{
  "name": "@ollierelph/result4t",
  "version": "0.5.6",

  "description": "",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/types/index.d.ts",
        "require": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "./dist/*",
    "package.json"
  ],
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.10.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "scripts": {
    "test": "pnpm vitest",
    "build": "rm -rf ./dist || true && pnpm tsc -b ./tsconfig.build-cjs.json ./tsconfig.build-esm.json ./tsconfig.build-types.json",
    "format": "prettier --write ./",
    "publish-package": "echo \"$(jq --arg name \"${NAME_TO_PUBLISH:-@ollierelph/result4t}\" '.name|=$name' package.json)\" > package.json && pnpm publish --access public --no-git-checks"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BBB/result4t.git"
  },
  "keywords": [
    "fp",
    "result",
    "option",
    "monad",
    "typescript",
    "typesafe",
    "promises",
    "task",
    "node"
  ],
  "author": "Ollie Relph",
  "license": "MIT"
}
