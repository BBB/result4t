{
  "name": "@ollierelph/result4t",
  "version": "0.4.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/types/*.d.ts",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "default": "./dist/esm/*.js"
    }
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/node": "^18.0.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.0",
    "vitest": "^0.30.1"
  },
  "scripts": {
    "test": "pnpm vitest",
    "build": "rm -rf ./dist || true && pnpm tsc -b ./tsconfig.build-cjs.json ./tsconfig.build-esm.json ./tsconfig.build-types.json",
    "format": "prettier --write ./",
    "publish-package": "echo \"$(jq --arg name \"${NAME_TO_PUBLISH:-@ollierelph/result4t}\" '.name|=$name' package.json)\" > package.json && pnpm publish --access public --no-git-checks"
  },
  "files": [
    "./dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BBB/result4t.git"
  },
  "keywords": [
    "fp",
    "result",
    "option",
    "monad",
    "typescript",
    "typesafe",
    "promises",
    "task",
    "node"
  ],
  "author": "Ollie Relph",
  "license": "MIT"
}
